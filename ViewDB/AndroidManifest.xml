<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sw6.oasis.views"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="13" />

    <application
        android:icon="@drawable/elephant"
        android:label="@string/app_name" >
        <activity
            android:name=".StartUp"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="sw6.oasis.views.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="ViewApps" />
        <activity android:name="ViewDepartments" />
        <activity android:name="ViewMedia" />
        <activity android:name="ViewProfiles" />
        <activity android:name="ViewCertificates" />
        <activity android:name="ViewListOfApps" />
        <activity android:name="ViewAuthUsers" />
        <activity android:name="ViewHasDepartment" />
        <activity android:name="ViewHasGuardian" />
        <activity android:name="ViewHasLink" />
        <activity android:name="ViewHasSubDepartment" />
        <activity android:name="ViewHasTag" />
        <activity android:name="ViewMediaDepartmentAccess" />
        <activity android:name="ViewMediaProfileAccess" />
        <activity android:name="ViewTags" />
    </application>

</manifest>