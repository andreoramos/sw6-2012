<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sw6.autism.admin"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="13" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".TableList"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Apps"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="sw6.autism.admin.APPS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Departments"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="sw6.autism.admin.DEPARTMENTS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Media"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="sw6.autism.admin.MEDIA" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Profiles"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="sw6.autism.admin.PROFILES" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Settings"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="sw6.autism.admin.SETTINGS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Stats"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="sw6.autism.admin.STATS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <provider
            android:name="sw6.autism.admin.provider.DbProvider"
            android:authorities="sw6.autism.admin.provider.AutismProvider" >
        </provider>
    </application>

</manifest>